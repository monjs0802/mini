 <mapper namespace="noticeSQL">
	<insert id="noticeWrite" parameterType="notice">
 		insert into noticeBoard values(seq_notice.nextval
							          ,#{noticeSubject}
							  		  ,#{noticeContent}
							  		  ,0
							  	      ,sysdate)
